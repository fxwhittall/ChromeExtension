{
  "name": "Black background",
  "version": "0.0.1",
  "description": "Changes the background color of google.com to black",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "./index.html",
    "default_title": "Open the popup",
    "default_icon":"./icon16.png"
  },
    "content_scripts": [
        {
            "js": ["content.js"],
            "css": ["content.css"],
            "matches": ["https://*.google.com/*"]
        }
    ],
    "background":[
      {
        "scripts": ["background.js"]
      }
    ],
    "web_accessible_resources":[
      "Button_Push.mp3"
    ]
}